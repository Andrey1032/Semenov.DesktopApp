   <string notr="true">/* Основные настройки шрифта */
* {
    font-family: Arial, Helvetica, sans-serif;
}

/* Окружающая среда главного окна */
#main-window,
#practice2-window {
    background-color: #f8f8ff;
    padding: 20px;
}

/* Ярлык (label) */
QLabel {
    color: #333;
    font-size: 14px;
    margin-bottom: 10px;
}

/* Поля ввода (spin-box) */
QSpinBox {
    width: 100px;
    height: 30px;
    border-radius: 5px;
    border: 1px solid #ccc;
    padding-left: 10px;
}

/* Кнопки */
QPushButton {
    background-color: #4CAF50;
    color: white;
    border-radius: 5px;
    padding: 10px 20px;
    min-width: 100px;
}

QPushButton:hover {
    background-color: #45A049;
}

/* Таблица (для задачи коммивояжера) */
QTableWidget {
    border-collapse: collapse;
    selection-background-color: #4CAF50;
    alternate-background-color: #fafafa;
    gridline-color: #ddd;
}

QTableWidget::item {
    padding: 5px;
    text-align: center;
}

QHeaderView::section {
    background-color: #f8f8ff;
    border: none;
    padding-right: 10px;
    color: #333;
}

/* Элемент вывода результата (текстовая область) */
QTextEdit {
    background-color: #fff;
    border: 1px solid #ccc;
    border-radius: 5px;
    padding: 10px;
    min-height: 100px;
}

/* Канва графика (для задачи кластеризации) */
#plot-canvas {
    max-height: 400px;
    max-width: 600px;
    background-color: white;
    border: 1px solid #ddd;
    box-shadow: 0 0 10px rgba(0,0,0,.1);
}</string>
